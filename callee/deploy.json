{
  "id": "/python-app-group3",
  "groups": [
    {
      "id": "/python-app-group3/app-group1",
      "apps": [
        {
          "id": "basic-3",
          "labels": {"HAPROXY_GROUP": "external"},
	  "args": ["python", "callee.py", "2000", "5"],
          "cpus": 0.25,
          "mem": 64,
          "ports": [
           10002
          ],
          "container": {
            "type": "DOCKER",
            "docker": {
              "image": "registry.global.gravity/vivek/callee:9",
              "network": "BRIDGE",
              "portMappings": [
                {
                  "containerPort": 2000,
                  "hostPort": 0,
		  "servicePort": 10002
                }
              ]
            }
          },
          "healthChecks": [
            {
              "path": "/isHealthy",
              "protocol": "HTTP",
              "portIndex": 0,
              "command": null,
         "timeoutSeconds": 10,
              "maxConsecutiveFailures": 1,
              "gracePeriodSeconds": 15,
              "ignoreHttp1xx": false
            }
          ]
        }
      ]
    }
  ]
}
